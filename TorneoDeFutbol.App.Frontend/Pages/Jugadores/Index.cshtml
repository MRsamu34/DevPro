@page
@model TorneoDeFutbol.App.Frontend.Pages.Jugadores.IndexModel
@{
}
<h1>Listados de jugadores</h1>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Nombres</th>
      <th scope="col">Numero</th>
      <th scope="col">Posicion</th>
      <th scope="col">Acciones</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var jugador in Model.jugador)
    {
      <tr>
        <th>@jugador.Nombre</th>
        <td>@jugador.Numero</td>
        <td>@jugador.Posicion</td>
      </tr>
      <td>
        <a class="btn btn-primary" asp-page="./Details" asp-route-id="@jugador.Id">Detalle</a>
      </td>
    }
  <tbody>
</table>